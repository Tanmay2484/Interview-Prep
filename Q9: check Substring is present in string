#include <bits/stdc++.h>
using namespace std;

int main() {
    string s,sub;
    cin >> s>>sub;
    int check=0;
    for(int i=0;i<s.size()-sub.size();i++){
        if(s.substr(i,sub.size())==sub){
            check=1;
        }
    }
    if(check) 
        cout<<"Yes";
    else 
        cout<<"No";
    
    return 0;
}
